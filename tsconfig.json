{
  "compilerOptions": {
    "target": "es5",                             
    "module": "commonjs",
    "experimentalDecorators": true,              
    "emitDecoratorMetadata": true,               
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,                                                           
    "skipLibCheck": true,
    "resolveJsonModule": true                      
  }
}
